{
    "name": "BadApple - Moodle UFABC",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "BadApple"
    },
    
    "background": {
        "scripts": ["background.js"]
    }
}
